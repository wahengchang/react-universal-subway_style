{
  "name": "react-universal-subway",
  "version": "1.0.0",
  "description": "react redux unviersal subway-style boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "rimraf public && better-npm-run build",
    "start": "npm run build && better-npm-run start:prod",
    "dev": "rimraf public && better-npm-run start"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node ./src/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start:prod": {
      "command": "node ./src/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": "https://www.github.com/rahulbasu710",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel",
    "universal",
    "router",
    "JWT",
    "auth0"
  ],
  "author": "Rahul Basu",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "compression": "^1.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "express": "^4.16.1",
    "grommet": "^1.8.2",
    "helmet": "^3.8.2",
    "hpp": "^0.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
